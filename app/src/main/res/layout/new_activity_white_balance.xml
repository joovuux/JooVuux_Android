                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="3"
                            android:id="@+id/tvNumberSharpness1"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
