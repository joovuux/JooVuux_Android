                        app:max="12"
                        app:pointer_alpha_ontouch="100"
                        app:pointer_color="#149df00"
                        app:pointer_halo_color="#149df00"

                        app:circle_progress_color="#ff48d8b7"
                        app:circle_color="#ffa8a8a8"

                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true"
                        android:visibility="visible"
                        android:clickable="false" />

                    <com.joovuux.circularseekbar.DoughterCircularSeekBar
