        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:baselineAligned="false"
        android:id="@+id/linearLayout10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <RelativeLayout
