                    android:id="@+id/relativeLayout5Saturation">

                    <SeekBar
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/seekBarWBSaturation"
                        android:max="256"/>

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#fff"></FrameLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="|"
                            android:id="@+id/tvLineSaturation0"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="|"
                            android:id="@+id/tvLineSaturation1"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"
                            android:textSize="16dp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="|"
                            android:id="@+id/tvLineSaturation2"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="right"
                            android:textStyle="bold"
                            android:textSize="16dp" />

                    </LinearLayout>

                    <com.joovuux.CustomProgressBar
                        android:id="@+id/progressSaturation"
                        style="@style/CustomProgressBar"
                        android:layout_width="match_parent"
                        android:layout_height="18dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:progress="64"
                        android:max="256"
                        android:layout_marginLeft="2dp" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/progressSaturation"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="5dp">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="0"
                            android:id="@+id/tvNumberSaturation0"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="left"
                            android:textStyle="bold"
                            android:textSize="11dp" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="128"
                            android:id="@+id/tvNumberSaturation1"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"
                            android:textSize="11dp"
                            android:textIsSelectable="false" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="256"
                            android:id="@+id/tvNumberSaturation2"
                            android:layout_weight="1"
                            android:textColor="#ff48d8b7"
                            android:gravity="right"
                            android:textStyle="bold"
                            android:textSize="11dp" />

                    </LinearLayout>

                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>